{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": null,
  "functions": {
    "src/api/handler.js": {
      "runtime": "nodejs22.x"
    }
  },
  "routes": [
    {
      "src": "/graphql",
      "dest": "/src/api/handler"
    },
    {
      "src": "/health",
      "dest": "/src/api/handler"
    },
    {
      "src": "/",
      "dest": "/src/api/handler"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
