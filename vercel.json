{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": null,
  "routes": [
    {
      "src": "/graphql",
      "dest": "/src/api/handler"
    },
    {
      "src": "/health", 
      "dest": "/src/api/handler"
    },
    {
      "src": "/",
      "dest": "/src/api/handler"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
